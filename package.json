{
  "name": "react-redux-starter",
  "description": "react-redux-starter",
  "scripts": {
    "start": "npm run watch:js & npm run serve -- --files 'src/**/*{.jsx,.js}' --server public",
    "serve": "browser-sync start --host 127.0.0.1 --open external --no-notify",
    "watch:js": "watchify src/index.js -v -t [ babelify --stage 1 ] --extension='.jsx' -o public/index.js"
  },
  "author": "sugarshin",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^5.8.29",
    "classnames": "^2.2.0",
    "react": "~0.14.0",
    "react-dom": "~0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babelify": "^6.4.0",
    "browser-sync": "^2.9.11",
    "browserify": "^11.2.0",
    "envify": "^3.4.0",
    "watchify": "^3.5.0"
  }
}
